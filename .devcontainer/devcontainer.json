{
	"name": "Node.js Sample",
	"dockerFile": "Dockerfile",
	"appPort": [
		3000,
		1235
	],
	"extensions": [
		"dbaeumer.vscode-eslint"
	],
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash"
	},
	// Comment out the next line to run as root instead. Linux users, update
	// Dockerfile with your user's UID/GID if not 1000.
	"runArgs": [
		"-u",
		"node"
	]
}